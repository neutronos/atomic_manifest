<?xml version="1.0" encoding="UTF-8"?> <manifest>
 
  <remote name="gitlab"
          fetch="https://gitlab.com/" />

  <remote name="github"
           fetch="https://github.com/" />

  <remote  name="stag"
           revision="r11"
           fetch="https://github.com/StagOS/"  />

  <remote  name="gitlab-stag"
